            "dms/modules/dictionary/orgstructure/*": [
                "node_modules/@ctp.cmf/cmf-dms-dictionary-orgstructure-ui/dist/dms/modules/dictionary/orgstructure/*"
            ],
            "dms/modules/security/*": [
                "node_modules/@ctp.cmf/cmf-dms-security-ui/dist/dms/modules/security/*"
            ],
            "dms/modules/task/*": [
                "node_modules/@ctp.cmf/cmf-dms-task-ui/dist/dms/modules/task/*"
            ],
            "dms/modules/mainPage/*": [
                "node_modules/@ctp.cmf/cmf-dms-mainpage-ui/dist/dms/modules/mainPage/*"
            ],
            "dms/modules/document/regnumber/*": [
                "node_modules/@ctp.cmf/cmf-dms-document-regnumber-ui/dist/dms/modules/document/regnumber/*"
            ],
            "dms/modules/report/*": [
                "node_modules/@ctp.cmf/cmf-dms-report-ui/dist/dms/modules/report/*"
            ],
            "dms/modules/comment/*": [
                "node_modules/@ctp.cmf/cmf-dms-comment-ui/dist/dms/modules/comment/*"
