            "dms/modules/dictionary/security/*": [
                "node_modules/@ctp.cmf/cmf-dms-dictionary-security-ui/dist/dms/modules/dictionary/security/*"
            ],
            "dms/modules/dictionary/user/*": [
                "node_modules/@ctp.cmf/cmf-dms-dictionary-user-ui/dist/dms/modules/dictionary/user/*"
            ],
            "dms/modules/dictionary/employee/*": [
                "node_modules/@ctp.cmf/cmf-dms-dictionary-employee-ui/dist/dms/modules/dictionary/employee/*"
            ],
            "dms/modules/dictionary/positionDescription/*": [
                "node_modules/@ctp.cmf/cmf-dms-dictionary-position-description-ui/dist/dms/modules/dictionary/positionDescription/*"
            ],
            "dms/modules/dictionary/role/*": [
                "node_modules/@ctp.cmf/cmf-dms-dictionary-role-ui/dist/dms/modules/dictionary/role/*"
