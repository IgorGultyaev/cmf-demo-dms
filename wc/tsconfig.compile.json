                "node_modules/@ctp.cmf/cmf-dms-document-approval-ui/dist/dms/modules/document/approval/*"
            ],
            "dms/modules/document/process/*": [
                "node_modules/@ctp.cmf/cmf-dms-document-process-ui/dist/dms/modules/document/process/*"
            ],
            "dms/modules/document/*": [
                "node_modules/@ctp.cmf/cmf-dms-document-ui/dist/dms/modules/document/*"
            ],
            "dms/modules/commission/*": [
                "node_modules/@ctp.cmf/cmf-dms-commission-ui/dist/dms/modules/commission/*"
            ],
            "dms/modules/search/*": [
                "node_modules/@ctp.cmf/cmf-dms-search-ui/dist/dms/modules/search/*"
            ],
            "lib/*": [
                "node_modules/@croc/webclient/dist/lib/*"
            ],
            "modules/*": [
                "node_modules/@croc/webclient/dist/modules/*"
            ],
            "vendor/*": [
                "node_modules/@croc/webclient/dist/vendor/*"
            ],
            "core": [
                "node_modules/@croc/webclient/dist/lib/core"
            ],
            "moment": [
                "node_modules/@croc/webclient/dist/vendor/moment/moment"
            ],
            "Draggable": [
                "node_modules/@shopify/draggable/lib/es5/draggable.bundle.legacy.js"
            ],
            "chartjs": [
                "node_modules/chart.js/dist/Chart.js"
            ],
            "pdfjs-dist/build/pdf": [
                "node_modules/@ctp.cmf/cmf-ui/dist/cmf/vendor/pdfjs-dist/build/pdf"
            ]
        }
    },
    "include": [
        "src/**/*.ts",
        "src/**/.*.ts"
    ],
    "compileOnSave": true
}
