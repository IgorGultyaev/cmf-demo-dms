{
    "extends": "./node_modules/@croc/webclient/tsconfig",
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "module": "amd",
        "target": "es3",
        "sourceMap": true,
        "declaration": false,
        "noEmitHelpers": true,
        "experimentalDecorators": true,
        "skipLibCheck": true,
        "skipDefaultLibCheck": true,
        "baseUrl": ".",
        "rootDir": ".",
        "outDir": "build",
        "paths": {
            "app/*": [
                "src/app/*"
            ],
            "cmf/modules/dashboard/*": [
                "node_modules/@ctp.cmf/cmf-dashboard-ui/dist/cmf/modules/dashboard/*"
            ],
            "cmf/modules/calendar/*": [
                "node_modules/@ctp.cmf/cmf-calendar-ui/dist/cmf/modules/calendar/*"
            ],
            "cmf/*": [
                "node_modules/@ctp.cmf/cmf-ui/dist/cmf/*"
            ],
            "dms/modules/icons/*": [
                "node_modules/@ctp.cmf/cmf-dms-appicon-ui/dist/dms/modules/icons/*"
            ],
            "dms/modules/bookmarks/*": [
                "node_modules/@ctp.cmf/cmf-dms-bookmarks-ui/dist/dms/modules/bookmarks/*"
            ],
            "dms/modules/info/*": [
                "node_modules/@ctp.cmf/cmf-dms-info-ui/dist/dms/modules/info/*"
            ],
            "dms/modules/dictionary/extension/*": [
                "node_modules/@ctp.cmf/cmf-dms-dictionary-model-extension-ui/dist/dms/modules/dictionary/extension/*"
